[Options]
# Defualt workspaces
workspaces := www irc src

# Default layout (Floating or Tiling)
default_layout := Floating

# Focus behavoir
focus_follows_mouse := yes
focus_follows_mouse_raise := no
focus_follows_mouse_head := no

# Floating padding (when spawning windows under the cursor)
floating_padding := 49 20 20 20

# Exec shell
shell := dash
